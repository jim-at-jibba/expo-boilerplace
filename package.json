{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "check-formatting": "prettier --config .prettierrc 'src/**/*.ts' '**/*.ts' --list-different",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' '**/*.ts' --write",
    "lint-src": "eslint 'src/**' --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint-src && npm run check-ts-types",
    "check-ts-types": "tsc -p ./tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "expo": "~43.0.2",
    "expo-status-bar": "~1.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-universe": "^8.0.0",
    "fit-commit-js": "^0.3.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "typescript": "~4.3.5"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint --ext .js,.ts,.tsx,.jsx",
      "prettier --list-different"
    ]
  }
}
